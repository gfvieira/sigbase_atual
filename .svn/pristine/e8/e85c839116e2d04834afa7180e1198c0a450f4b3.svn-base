html {
	width: 100%;
	height: 100%;
	padding: 0px;
	margin: 0px;
}

body {
	width: 100%;
	height: 100%;
	position: relative;
	padding: 0px;
	margin: 0px;
	font: 14/1.21 'Lato','Minion Pro','Crimson Text',serif,tahoma,arial,helvetica,clean,sans-serif  !important;
	color: #333 !important;
}

p {
	font: 14/1.21 'Lato','Minion Pro','Crimson Text',serif,tahoma,arial,helvetica,clean,sans-serif  !important;
    color: #333 !important;
}

fieldset {
    border: none;
}

/* links */
a {
	text-decoration: none;
}

a:link {
	color: #0e575e;
}

a:hover {
	color: #0e575e;
}

a:visited {
	color: #0e575e;
}

a:active {
	color: #0e575e;
}

h1,h2,h3,h4,h5,h6 {
	font-family: 'Lato', sans-serif;
	color: #0e575e;
	color: #0e575e;
	color: #0e575e;
}

h1 {
	font: 13px/1.231
}

h2 {
	font-size: 1.0em;
}

.breadcrumb {
	border-bottom: 0.1em solid black;
}

h1 {
	font: 14/1.21 'Lato','Minion Pro','Crimson Text',serif,tahoma,arial,helvetica,clean,sans-serif  !important;
	font-weight:bold !important;
}


#page {
	width: 100%;
	margin: 0 auto;
	padding: 0px;
}

#content {
	padding-top: 100px;
	padding-bottom: 50px;
	width: 70%;
	text-align: center;
	margin-left: 15%;
	margin-right: 15%;
	overflow-y: auto;
}

#col1 {
	float: left;
	width: 50%;
	
}

#col2 {
	float: right;
	width: 50%;
}

#col3 {
	width: 100%;
	text-align: left;
}

#footer {
	position: fixed;
	text-align: center;
	vertical-align: center;
	bottom: 0px;
	height: 30px;
	width: 100%;
	background-color: #fff;
	font-weight: bold;
	border-top: 1px solid black;
	z-index: 9999;
	font: 13px/1.231 'Lato', 'Minion Pro', 'Crimson Text', serif, tahoma,
		arial, helvetica, clean, sans-serif !important;
	padding: 0px;
}

#footer p {
	padding: 0px;
	margin: 0px;
}

#header_banner {
	z-index: 1;
	background-color: #0e2d5e;
	color: #fff;
	text-align: left;
	height: 90px;
	font: 13px/1.231 'Lato', 'Minion Pro', 'Crimson Text', serif, tahoma, arial,
		helvetica, clean, sans-serif !important;
	position: fixed;
	top: 0px;
	width: 100%;
	z-index: 9999;
	padding: 0px;
	margin: 0px;
}

.bannerLeft {
	position: absolute;
	height: 64px;
	width: 100%;
	background-image: url(/oscar/img/top-menu-bg2.png);
}

.bannerRight {
	position: absolute;
	right: 0px;
	top: 0px;
	width: 268px;
	height: 64px;
	background-image: url(/oscar/img/top-menu-right.png);
}

.canvas {
	position: relative;
	padding-bottom: 0px;
	z-index: 9999;
	height: 100%;
	width: 50%;
	margin-left: 25%;
}

.canvas .home {
	margin-top: 0;
	margin-bottom: 40px;
	padding-left: 1em;
	color: #0e2d5e;
	font-size: 1em;
	font-family: 'Lato', sans-serif;
}

.tab,.tabactive {
	font-family: 'Lato', sans-serif;
	font-size: 0.90em;
	font-weight: 700;
	display: inline-block;
	width: 145px;
	height: 34px;
	text-align: center;
	padding-top: 7px;
	margin: 0px;
	-moz-border-radius-right-top: 4px;
}

.tabactive {
	background-image: url(/oscar/img/tab-bg_n.png);
	background-repeat: no-repeat;
}

.tab:hover {
	background-image: url(/oscar/img/tab-bg_o.png);
	background-repeat: no-repeat;
}

.tab a {
	color: #fff;
}

/* map styles */
.smallMap {
	width: 640px;
	height: 300px;
	border: 1px solid #ace;
}

.mapDiv {
    width: 80%;
    height: 95%;
    float: left;
    position: relative;
}

.mediumMap {
	/*width: 590px;
	height: 540px;
	border: 1px solid #ace;
	position: relative;
    float: left;
    margin: 0px 0px 0px 10px;*/
}

/* graph container */

#container {
    float: left;
    position:relative;
    z-index:3000;
    margin-left:-600px;
    margin-top:0px;
    padding-right:0px;
    padding-bottom:20px;
    padding-top:10px;
    width:590px;
    opacity:0.9;
    
    /*min-width: 590px;
    height: 540px;
    float: right;*/
}

/* logo panel */

#logo_panel {
	width:200px;
	height:102px; 
    float:right;
	background-image:url("../images/logo.gif");
    background-repeat:no-repeat;
	margin: 25px 0px 10px 0px;
    }
    
/* input */
#sidebar{
	position:absolute;
	top:0px;
	right:0px;
	text-align:center;
	border:1px solid #ddd;
	padding:20px;
	width:15%;
}
#sidebar table td.first-child{
	width:150px;
}
#fields {
	text-align:left;
	margin:0 auto;
}
#fields input[type="text"]{
	width:50px;
}
#fields select{
	width:160px;
}

/* examples */
.examplesList {
	width: 100%;
	vertical-align: text-top;
}

.example {
	vertical-align: text-top;
	display: inline-block;
	width: 225px;
	height: 130px;
	text-align: center;
	border: 1px solid black;
	margin: 4px 4px 4px 4px;
	-moz-border-radius: 4px;
	overflow: auto;
}

#wrap {
	max-width: 960px;
	margin: 0 auto;
	padding: 0 20px;
	z-index: 0;
	border-top: solid 15px #f0f0f0;
	}